
import java.util.*; 

class Main {

    public static void main(String args[]) {
       Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        for(int row=1;row<=n;row++){
            for(int i=1;i<=n;i++){
                if(n-row>=i){
                    System.out.print("  ");
                }
                else System.out.print("* ");
            }
            System.out.println();
    }
         
    }}
