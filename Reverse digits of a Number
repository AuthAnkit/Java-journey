import java.util.*;

public class Main {
    public static void main(String[] args) {
    
    Scanner sc = new Scanner(System.in);
    int N = sc.nextInt();
    int nn=0;
    while(N/10!=0){
        nn=N%10; 
        System.out.print(nn);
        N=N/10;
    }
    System.out.print(N);

    }
    
    }
