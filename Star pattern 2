import java.util.*; 
class Main {

    public static void main(String args[]) {

          Scanner sc = new Scanner(System.in);
          int n = sc.nextInt();

        for(int space =0;space<n;space++){
            for(int star=n;star>space;star--){
                System.out.print("*"+" ");
            }
            System.out.println();
        }
          
      }
    
}
